@using UserPtoject.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model List<User>

<table>
    <thead>
        <tr>

            <th>İsim</th>
            <th>Soyisim</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <th>@item.Name</th>
                <th>@item.LastName</th>
                <th><a asp-action="Update" asp-controller="User" asp-route-id=@item.Id>Güncelle</a></th>
                <th><a asp-action="Remove" asp-controller="User" asp-route-id=@item.Id>Sil</a></th>
            </tr>
        }

    </tbody>

</table>

<a asp-action="Create" asp-controller="User">Yeni Kullanıcı Ekle</a>